<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.hellofx.seleccionarPerfilFiscalController">
   <top>
      <Label text="Elegir Pefil Fiscal:" BorderPane.alignment="CENTER" />
   </top>
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="tableView" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <columns>
                  <TableColumn fx:id="idColumn" prefWidth="75.0" text="ID" />
                  <TableColumn fx:id="pacienteColumn" prefWidth="75.0" text="Paciente" />
                  <TableColumn fx:id="razonSocialColumn" prefWidth="75.0" text="Razon Social" />
                  <TableColumn fx:id="rfcColumn" prefWidth="75.0" text="RFC" />
                  <TableColumn fx:id="cpColumn" prefWidth="75.0" text="CP" />
                  <TableColumn fx:id="regimenColumn" prefWidth="75.0" text="Regimen" />
               </columns>
            </TableView>
         </children>
      </AnchorPane>
   </center>
   <bottom>
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="600.0" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="cancelButton" maxWidth="Infinity" mnemonicParsing="false" onAction="#closeWindow" text="Cancelar" HBox.hgrow="ALWAYS" />
            <Button maxWidth="Infinity" mnemonicParsing="false" onAction="#handleNewButton" text="Nuevo" HBox.hgrow="ALWAYS" />
            <Button maxWidth="Infinity" mnemonicParsing="false" onAction="#handleAcceptButton" text="Aceptar" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </bottom>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</BorderPane>
